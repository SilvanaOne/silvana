          "rawConfiguration": {
            "text": "# Silvana Coordinator Dashboard\n\n## Service Overview\nReal-time monitoring of the Silvana Coordinator service including job processing, container management, multicall batching, and system health."
          }
        },
        {
          "title": "Response Time (ms)",
          "layout": {
            "column": 1,
            "row": 3,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 7077955,
                "query": "SELECT average(http.server.duration) AS 'Response Time (ms)' FROM Metric WHERE service.name = 'silvana-coordinator' SINCE 1 hour ago TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Throughput (jobs/min)",
          "layout": {
            "column": 5,
            "row": 3,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 7077955,
                "query": "SELECT rate(sum(silvana.job.count), 1 minute) AS 'Throughput' FROM Metric WHERE service.name = 'silvana-coordinator' SINCE 1 hour ago TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
