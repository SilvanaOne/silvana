          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 7077955,
                "query": "SELECT percentage(count(silvana.job.errors), count(silvana.job.count)) AS 'Error Rate' FROM Metric WHERE service.name = 'silvana-coordinator' SINCE 30 minutes ago"
