    "template": "npm run test test/template.test.ts",
    "create": "npm run test test/create.test.ts",
    "sum": "npm run test test/sum.test.ts",
    "add": "npm run test test/add.test.ts",
    "batch": "npm run test test/batch.test.ts",
    "multiply": "npm run test test/multiply.test.ts",
    "purge": "npm run test test/purge.test.ts",
    "rollup": "npm run test test/rollup.test.ts",
    "commitment": "npm run test test/commitment.test.ts",
    "transition": "npm run test test/transition.test.ts",
    "deploy": "npm run test test/deploy.test.ts",
    "send": "npm run test test/send.test.ts",
    "compile": "npm run test test/compile.test.ts",
    "proto": "rm -rf src/proto && cd ../../../ && buf generate --template buf.gen.agent.yaml"
  },
  "node": {
    "version": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "dotenv": "^17.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.7.0",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@mysten/sui": "^1.37.6",
    "@silvana-one/coordination": "^1.0.31",
