        {
          "title": "Response Time (ms)",
          "layout": {
            "column": 1,
            "row": 3,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [7077955],
                "query": "SELECT average(http.server.duration) as 'Response Time (ms)' FROM Metric WHERE service.name = 'silvana-coordinator' TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Throughput (jobs/min)",
          "layout": {
            "column": 5,
            "row": 3,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [7077955],
                "query": "SELECT rate(sum(silvana.job.count), 1 minute) as 'Throughput' FROM Metric WHERE service.name = 'silvana-coordinator' TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Error Rate (%)",
          "layout": {
            "column": 9,
            "row": 3,
            "width": 4,
